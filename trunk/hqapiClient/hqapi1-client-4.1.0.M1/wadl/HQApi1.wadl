        <wadl:resource path="agent/ping.hqu">
            <wadl:doc title="HQApi Agent ping command">
                Ping an Agent
            </wadl:doc>
            <wadl:method name="GET" id="AgentPing">
                <wadl:request>
                    <wadl:param name="id" style="query" required="true"
                                type="xs:int">
                        <wadl:doc>
                            The id of the Agent to ping.
                        </wadl:doc>
