        <wadl:resource path="alert/find.hqu">
            <wadl:doc title="HQApi Alert find command">
                Find Alerts
            </wadl:doc>
            <wadl:method name="GET" id="AlertFind">
                <wadl:request>
                    <wadl:param name="begin" style="query" required="true"
                                type="xs:long">
                        <wadl:doc>
                            The beginning of the time window in epoch-millis.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="end" style="query" required="true"
                                type="xs:long">
                        <wadl:doc>
                            The end of the time window in epoch-millis.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="count" style="query" required="true"
                                type="xs:int">
                        <wadl:doc>
                            The maximum number of Alerts to return.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="severity" style="query" required="true"
                                type="xs:int">
                        <wadl:doc>
                            The minimum Alert severity to return.  1 = LOW,
                            2 = MEDIUM, 3 = HIGH.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="inEscalation" style="query" required="false"
                                type="xs:boolean">
                        <wadl:doc>
                            If true, only return Alerts which are in Escalation.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="notFixed" style="query" required="false"
                                type="xs:boolean">
                        <wadl:doc>
                            If true, only return Alerts which are not fixed.
                        </wadl:doc>
                    </wadl:param>
                </wadl:request>
                <wadl:response>
                    <wadl:representation mediaType="application/xml"
                                         element="AlertsResponse"/>
                </wadl:response>
            </wadl:method>
        </wadl:resource>

        <wadl:resource path="alert/findByResource.hqu">
            <wadl:doc title="HQApi Alert findByResource command">
                Find Alerts for the given Resource
            </wadl:doc>
            <wadl:method name="GET" id="AlertFindByResource">
                <wadl:request>
                    <wadl:param name="resourceId" style="query" required="true"
                                type="xs:int">
                        <wadl:doc>
                            The id of the Resource to query for Alerts.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="begin" style="query" required="true"
                                type="xs:long">
                        <wadl:doc>
                            The beginning of the time window in epoch-millis.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="end" style="query" required="true"
                                type="xs:long">
                        <wadl:doc>
                            The end of the time window in epoch-millis.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="count" style="query" required="true"
                                type="xs:int">
                        <wadl:doc>
                            The maximum number of Alerts to return.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="severity" style="query" required="false"
                                type="xs:int">
                        <wadl:doc>
                            The minimum Alert severity to return.  1 = LOW,
                            2 = MEDIUM, 3 = HIGH.  Default is 1.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="inEscalation" style="query" required="false"
                                type="xs:boolean">
                        <wadl:doc>
                            If true, only return Alerts which are in Escalation.
                        </wadl:doc>
                    </wadl:param>
                    <wadl:param name="notFixed" style="query" required="false"
                                type="xs:boolean">
                        <wadl:doc>
                            If true, only return Alerts which are not fixed.
                        </wadl:doc>
                    </wadl:param>
                </wadl:request>
                <wadl:response>
                    <wadl:representation mediaType="application/xml"
                                         element="AlertsResponse"/>
                </wadl:response>
            </wadl:method>
        </wadl:resource>

